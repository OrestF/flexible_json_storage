<p id="notice"><%= notice %></p>

<h1>Api Bins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_bins.each do |api_bin| %>
      <tr>
        <td><%= link_to 'Show', api_bin %></td>
        <td><%= link_to 'Edit', edit_api_bin_path(api_bin) %></td>
        <td><%= link_to 'Destroy', api_bin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Bin', new_api_bin_path %>
